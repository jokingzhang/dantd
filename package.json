{
  "name": "dantd",
  "version": "1.0.10",
  "description": "react components by antdv3",
  "author": "joking_zhang <496691544@qq.com> (jokingzhang.com)",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
      "dist",
      "lib",
      "es"
  ],
  "keywords": [
      "ant-design",
      "component",
      "components",
      "framework",
      "frontend",
      "react",
      "react-component",
      "ui"
  ],
  "repository": {
      "type": "git",
      "url": "https://github.com/jokingzhang/dantd"
  },
  "engines": {
      "node": ">=8",
      "npm": ">=5"
  },
  "scripts": {
    "start": "PORT=3033 react-app-rewired start",
    "test": "jest --no-cache",
    "dev:build": "react-app-rewired build",
    "dev:publish": "react-app-rewired build && node ./scripts/publish.js",
    "doc:start": "father doc dev -p 3022",
    "doc:build": "father doc build",
    "doc:deploy": "father doc deploy",
    "lib:build": "father build && node ./scripts/moveDeclare.js && node ./scripts/changeLess2Css.js && rm -rf ./dist/entry"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "antd": ">=3.21.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.1",
    "babel-plugin-import": "^1.13.0",
    "colors": "^1.4.0",
    "customize-cra": "^0.9.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^3.0.9",
    "jest": "24.9.0",
    "jest-html-reporter": "^2.8.0",
    "klaw-sync": "^6.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "24.1.0",
    "father": "^2.29.2"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.2.1",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/parser": "^2.17.0",
    "antd": "^3.21.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "dantd": "^1.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix src/**/*.{ts,tsx}",
      "jest --bail --coverage --findRelatedTests",
      "git add ."
    ],
    "*.{md,css,html,less}": [
      "prettier --write",
      "git add ."
    ]
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ]
}
